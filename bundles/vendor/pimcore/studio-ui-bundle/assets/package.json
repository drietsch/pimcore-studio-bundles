{
  "name": "@pimcore/studio-ui-bundle",
  "version": "0.0.1",
  "scripts": {
    "dev-server": "NODE_OPTIONS=--max_old_space_size=4096 encore dev-server --port 3030",
    "dev": "npm run dev-vendor && npm run dev-core-dll && encore dev && npm run dev-sdk",
    "dev-sdk": "encore dev --config webpack.sdk.config && tsc-alias",
    "dev-vendor": "encore dev --config webpack.vendor.config",
    "dev-core-dll": "encore dev --config webpack.core-dll.config",
    "watch": "encore dev --watch",
    "build": "NODE_OPTIONS=--max_old_space_size=4096 npm run build-vendor && NODE_OPTIONS=--max_old_space_size=4096 npm run build-core-dll && encore production && npm run build-sdk",
    "build-sdk": "encore production --config webpack.sdk.config && tsc-alias",
    "build-vendor": "encore production --config webpack.vendor.config",
    "build-core-dll": "encore production --config webpack.core-dll.config",
    "build-storybook": "storybook build --output-dir ./storybook-static",
    "build-api-client": "npx @rtk-query/codegen-openapi ./build/api/openapi-config.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./js",
    "lint-fix": "eslint --ext .js,.jsx,.ts,.tsx ./js --fix",
    "test": "jest ./js/src --passWithNoTests",
    "storybook": "storybook dev -p 6006 --no-open",
    "generate-icons": "npx ts-node -O '{\"module\": \"CommonJS\", \"esModuleInterop\": true}' -r tsconfig-paths/register build/generate-icon-index.ts"
  },
  "exports": {
    ".": {
      "import": "./dist/sdk/main.js",
      "require": "./dist/sdk/main.js",
      "types": "./dist/build/types/src/sdk/main.d.ts"
    },
    "./*": {
      "import": "./dist/sdk/*/index.js",
      "require": "./dist/sdk/*/index.js",
      "types": "./dist/build/types/src/sdk/*/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "devDependencies": {
    "@babel/preset-react": "^7.25.9",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@rtk-query/codegen-openapi": "^1.2.0",
    "@storybook/addon-a11y": "^8.4.4",
    "@storybook/addon-essentials": "^8.4.4",
    "@storybook/addon-interactions": "^8.4.4",
    "@storybook/addon-links": "^8.4.4",
    "@storybook/addon-onboarding": "^8.4.4",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.5",
    "@storybook/blocks": "^8.0.5",
    "@storybook/react": "^8.0.5",
    "@storybook/react-webpack5": "^8.4.4",
    "@storybook/test": "^8.0.5",
    "@svgr/webpack": "^8.1.0",
    "@symfony/webpack-encore": "^4.7.0",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/webpack-env": "^1.18.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-webpack-plugin": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-refresh": "^0.14.2",
    "storybook": "^8.4.4",
    "stream": "^0.0.2",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3",
    "webpack-bundle-analyzer": "^4.10.2",
